function Main()
    Ax = gg.choice({
    "メッキ",
    "馬力",
    "バンパー、移植",
    "鬼キャン",
    "車出現",
    "戻る"}, nil, "車関係")

    if Ax ~= nil then

    if Ax == 1 then
    local api = gg.makeRequest("https://raw.githubusercontent.com/Hazimeteotukai/hazimeteotukaijanruwakecode/main/ジャンル分けスクリプトメイン/ジャンル分け数値/車関係チート/メッキ");
    pcall(load(api.content))
    end

    if Ax == 2 then
    local api = gg.makeRequest("https://raw.githubusercontent.com/Hazimeteotukai/hazimeteotukaijanruwakecode/main/ジャンル分けスクリプトメイン/ジャンル分け数値/車関係チート/馬力");
    pcall(load(api.content))
    end

    if Ax == 3 then
    local api = gg.makeRequest("https://raw.githubusercontent.com/Hazimeteotukai/hazimeteotukaijanruwakecode/main/ジャンル分けスクリプトメイン/ジャンル分け数値/車関係チート/パーツ");
    pcall(load(api.content))
    end

    if Ax == 4 then

    end

    if Ax == 5 then

    end

    if Ax == 6 then
    local api = gg.makeRequest("https://raw.githubusercontent.com/Hazimeteotukai/hazimeteotukaijanruwakecode/main/ジャンル分けスクリプトメイン/ジャンル分けスクリプトメイン")
    pcall(load(api.content))
    end
end

while true do
    if gg.isVisible(true) then
    Lext=1
    gg.setVisible(false)
    end
    if Lext == 1 then
        Main()
        Lext = -1
    end
end
